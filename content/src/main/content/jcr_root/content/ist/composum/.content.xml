<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cpl="http://sling.composum.com/platform/1.0"
        xmlns:cpp="http://sling.composum.com/pages/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="cpp:Site">
    <jcr:content
            jcr:primaryType="cpp:SiteConfiguration"
            jcr:mixinTypes="[mix:versionable]"
            sling:resourceType="composum/pages/stage/edit/site"
            cpl:last-publicMode="inPlace"
            jcr:description="the site about the Composum project"
            jcr:title="Composum Site"
            template="/apps/ist/composum/templates/site/composum">
        <style
                jcr:primaryType="nt:unstructured"
                category.edit="sites.ist.composum.edit"
                category.view="sites.ist.composum.view"
                themeFilter="^ist/composum/.*$"/>
        <thumbnail
                jcr:primaryType="nt:unstructured">
            <image
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="composum/pages/components/element/image"
                    imageRef="/content/ist/composum/assets/general/composum-lt.png"/>
        </thumbnail>
        <logo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="composum/pages/components/element/image"
                imageRef="/content/ist/composum/assets/general/Composum-color.svg"/>
        <settings
                jcr:primaryType="nt:unstructured">
            <google
                    jcr:primaryType="nt:unstructured">
                <api
                        jcr:primaryType="nt:unstructured"/>
            </google>
        </settings>
        <languages
                jcr:primaryType="nt:unstructured">
            <english
                    jcr:primaryType="nt:unstructured"
                    key="en"
                    label="english"/>
            <german
                    jcr:primaryType="nt:unstructured"
                    key="de"
                    label="deutsch"/>
        </languages>
    </jcr:content>
    <assets/>
    <home/>
    <meta/>
    <sitemap/>
</jcr:root>
