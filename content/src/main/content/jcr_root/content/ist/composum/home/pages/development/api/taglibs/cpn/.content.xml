                            jcr:description="Embeds an anchor link tag"
                            jcr:title="anchor">
                        <attributes
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/attributes">
                            <attribute_N66260
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="if set, this is used instead of tag name 'a'."
                                    jcr:title="name"
                                    defaultValue="a"
                                    valueType="String"/>
                            <attribute_N66275
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="deprecated: use 'class' like a normal HTML tag"
                                    jcr:title="classes"
                                    valueType="java.lang.String"/>
                            <attribute_N66293
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="an EL expression that determines whether the tag is shown"
                                    jcr:title="test"
                                    valueType="String"/>
                        </attributes>
                        <examples
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/examples">
                            <example
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/example"
                                    jcr:description="Renders an anchor with the name given by model.anchor , if that is not empty. The title attribute is a dynamic attribute - it has no special function in the tag, but is just rendered into the output."
                                    exampleCode="&lt;cpn:anchor test=&quot;${not empty model.anchor}&quot; &#xd;&#xa;    name=&quot;${model.anchor}&quot; title=&quot;${model.title}&quot;/>"/>
                        </examples>
                    </tag_N66246>
                    <tag
                            jcr:primaryType="cpp:Container"
                            sling:resourceType="ist/composum/components/documentation/taglib/tag"
                            jcr:description="&lt;p>Declares the topmost resource bundle for i18n translation in the current requests resource bundle stack.&lt;/p>"
                            jcr:title="bundle">
                        <attributes
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/attributes">
                            <attribute
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="the Java Resource Bundle base name"
                                    jcr:title="basename"
                                    mandatory="{Boolean}true"
                                    valueType="String"/>
                        </attributes>
                        <examples
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/examples">
                            <example
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/example"
                                    jcr:description="&lt;p>selects the Resource Bundle with the base name 'ist-composum-site' as the topmost resource bundle in the requests processing; this resource bundle is now the first in the chain for translations&lt;/p>"
                                    exampleCode="&lt;cpn:bundle basename=&quot;ist-composum-site&quot;/>"/>
                        </examples>
                    </tag>
                    <tag_354224103
                            jcr:primaryType="cpp:Container"
                            sling:resourceType="ist/composum/components/documentation/taglib/tag"
                            jcr:description="&lt;p>Includes a clientlib reference (JS, CSS etc).&lt;/p>&lt;p>Client libraries are folders of sling:resourceType composum/nodes/commons/clientlib containing subfolders for each type, and declare dependencies to other client libraries. The tag can reference multiple client libraries if they are referenced by category, and if the client library/-ies have dependencies to other client libraries / resources. Thus, the tag can render as several link / script / img tags.&lt;/p>&lt;p>see: &lt;a href=&quot;/content/ist/composum/home/platform&quot;>Clientlibs&lt;/a>&lt;/p>"
                            jcr:title="clientlib">
                        <attributes
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/attributes">
                            <attribute1
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="&lt;p>nothing is rendered if the test attribute value is ‚false‘ (general condition)&lt;/p>"
                                    jcr:title="test"
                                    defaultValue="true"
                                    valueType="Boolean"/>
                            <attribute
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="&lt;p>the category of client library to be included; one of category or path is required but categories are preferred and more flexible&lt;/p>"
                                    jcr:title="category"
                                    valueType="String"/>
                            <attribute_1873384053
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="&lt;p>the repository path to reference the client library to be included; one of category or path is required&lt;/p>"
                                    jcr:title="path"
                                    valueType="String"/>
                            <attribute_296928212
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="&lt;p>type of rendered resources: css, js, link, img, png, jpg&lt;/p>"
                                    jcr:title="type"
                                    defaultValue="link"
                                    valueType="String"/>
                        </attributes>
                        <examples
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/examples">
                            <example
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/example"
                                    jcr:description="a typical HTML head snippet to include header link tags ans styles; the edit mode styles are included only if the 'editMode' check returns 'true'"
                                    exampleCode="&lt;cpn:clientlib type=&quot;link&quot; category=&quot;${model.viewClientlibCategory}&quot;/> &#xa;&lt;cpn:clientlib type=&quot;css&quot; category=&quot;${model.viewClientlibCategory}&quot;/> &#xa;&lt;cpn:clientlib type=&quot;css&quot; test=&quot;${model.editMode}&quot; category=&quot;${model.editClientlibCategory}&quot;/>"/>
                            <example_776538368
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/example"
                                    jcr:description="the analogous example for the include of clientlib JS code - typically at the end of the HTML body"
                                    exampleCode="&lt;cpn:clientlib type=&quot;js&quot; category=&quot;${model.viewClientlibCategory}&quot;/> &#xa;&lt;cpn:clientlib type=&quot;js&quot; test=&quot;${model.editMode}&quot; category=&quot;${model.editClientlibCategory}&quot;/> &#xa;"/>
                        </examples>
                    </tag_354224103>
                    <tag_1632415820
                            jcr:primaryType="cpp:Container"
                            sling:resourceType="ist/composum/components/documentation/taglib/tag"
                            jcr:description="&lt;p>Creates a JSP / Java variable of an instance of a Java Bean class (model class). The variable is declared only in the body of the component tag. Each variable of the same name in the specified scope if hided during tag processing and restored on the tags end.&lt;/p>"
                            jcr:title="component"
                            footer="If you want to use the declared variable in the attributes of the component tag itself use @{...} as expression boundary instead of the normal JSP-EL expression boundary (${...}). In this case the expression evaluation is not done by the compiled JSP code (variable is not available). The component tag itself is evaluating such delayed expressions dynamically on tag start.">
                        <attributes
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/attributes">
                            <attribute
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="the name of the variable; this name must be unique in the declared scope"
                                    jcr:title="var"
                                    mandatory="{Boolean}true"
                                    valueType="String"/>
                            <attribute_1476800206
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="the complete class name of the Java class of the instance to create"
                                    jcr:title="type"
                                    mandatory="{Boolean}true"
                                    valueType="String"/>
                            <attribute_119311365
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="&lt;p>the scope of the variable declaration; the variable is declared inside of the component tag body and in this specified scope.&lt;/p>&lt;ul>&lt;li>&lt;strong>page&lt;/strong>: JSP page scope (default)&lt;/li>&lt;li>&lt;strong>request&lt;/strong>: available in the page and all included components&lt;/li>&lt;li>&lt;strong>session&lt;/strong>: the variable is available during session lifetime&lt;/li>&lt;/ul>&lt;p>In each case the availability of the variable is limited by the tag boundaries.&lt;/p>"
                                    jcr:title="scope"
                                    defaultValue="page"
                                    valueType="String"/>
                            <attribute_447603190
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="&lt;p>If 'true' a new instance is created and set as variable. If 'false' the variable is created only of not existing in the specified scope and reused if always available with a compatible type.&lt;/p>&lt;p>In the 'page' scope the default is 'true' otherwise 'false'. So you can declare a variable in request scope for a set of components in the include tree which is declared in each component which needs this and shared nevertheless independently from the creating script.&lt;/p>"
                                    jcr:title="replace"
                                    valueType="Boolean"/>
                        </attributes>
                        <examples
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/examples">
                            <example
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/example"
                                    jcr:description="Provides an instance of the specified type as variable 'model' in the page scope."
                                    exampleCode="&lt;cpn:component var=&quot;model&quot; type=&quot;my.project.model.ComponentModel&quot;> &#xa;    &lt;cpn:text value=&quot;${model.description}&quot;/> &#xa;    ... &#xa;&lt;/cpn:component> &#xa;&lt;%-- 'model' not available outside the component tag --%>"/>
                            <example_1417669199
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/example"
                                    jcr:description="The declared variable 'model' is also available in the script code of the included component. If the script of the included component declares the the variable 'model' also in request scope and of the same type (and without 'replace=&quot;true&quot;') the instance declared here and its current state is shared with the 'child' component."
                                    exampleCode="&lt;cpn:component var=&quot;model&quot; type=&quot;my.project.model.ComponentModel&quot; scope=&quot;request&quot;> &#xa;    &lt;sling:include path=&quot;child&quot;/> &#xa;&lt;/cpn:component> &#xa;"/>
                        </examples>
                    </tag_1632415820>
                    <tag_N65556
                            jcr:primaryType="cpp:Container"
                            sling:resourceType="ist/composum/components/documentation/taglib/tag"
                            jcr:description="&lt;p>An extension of the sling:defineObjects tag.&lt;/p>&lt;p>&lt;/p>"
                            jcr:title="defineObjects">
                        <examples
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/examples">
                            <example
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/example"
                                    jcr:description="&lt;p>Creates additional variables:&lt;/p>&lt;p>&lt;ul>&lt;li>pagesAccessAuthor , pagesAccessPreview , pagesAccessPublic : boolean variables that are true if the request runs in access mode author / preview / public , respectively.&lt;/li>&lt;li>pagesModeNone, pagesModePreview, pagesModeEdit, pagesModeDevelop : boolean variables that are true if the request runs in display mode none, preview, edit or develop, respectively.&amp;nbsp;&lt;/li>&lt;/ul>&lt;/p>"
                                    exampleCode="&lt;cpn:defineObjects/>"/>
                        </examples>
                    </tag_N65556>
                    <tag_121274924
                            jcr:primaryType="cpp:Container"
                            sling:resourceType="ist/composum/components/documentation/taglib/tag"
                            jcr:description="&lt;p>The ‚div‘ tag is rendering a HTML div tag if the condition (‚test’ attribute) is ‚true‘.&amp;nbsp;&lt;/p>"
                            jcr:title="div">
                        <attributes
                                jcr:primaryType="cpp:Container"
                                sling:resourceType="ist/composum/components/documentation/taglib/tag/attributes">
                            <attribute
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="&lt;p>nothing (exception: the ‚body‘ is ‚true‘) is rendered if the test attribute value is ‚false‘ (general condition)&lt;/p>"
                                    jcr:title="test"
                                    defaultValue="true"
                                    valueType="Boolean"/>
                            <attribute_202944647
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="&lt;p>if the ‚body‘ condition is true the tags body is rendered even if the general ‚test‘ condition is ‚false’ - the 'conditional tag around' case&lt;/p>"
                                    jcr:title="body"
                                    defaultValue="false"
                                    valueType="Boolean"/>
                            <attribute_1267069690
                                    jcr:primaryType="cpp:Element"
                                    sling:resourceType="ist/composum/components/documentation/taglib/tag/attribute"
                                    jcr:description="&lt;p>the HTML tag name; if ‚none‘ no tag is rendered around the body&lt;/p>"
                                    jcr:title="tagName"
                                    defaultValue="div"
                                    valueType="String"/>
                            <attribute_137007149
