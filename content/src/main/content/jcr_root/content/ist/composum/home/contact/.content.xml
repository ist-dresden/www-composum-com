<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cpp="http://sling.composum.com/pages/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="cpp:Page">
    <jcr:content
            jcr:primaryType="cpp:PageContent"
            sling:resourceType="composum/pages/components/page"
            jcr:description="Ways to contact us"
            jcr:title="Contact"
            alternatives="/contact.html"
            template="ist/composum/templates/page/content">
        <top
                jcr:primaryType="cpp:Container"
                sling:resourceType="composum/pages/components/container/parsys">
            <title
                    jcr:primaryType="cpp:Element"
                    sling:resourceType="composum/pages/components/element/title"
                    title="Contact us">
                <image
                        jcr:primaryType="nt:unstructured"
                        imageRef="/content/ist/software/assets/background/gravitational_waves.jpg"/>
            </title>
        </top>
        <main
                jcr:primaryType="cpp:Container"
                sling:resourceType="composum/pages/components/container/parsys">
            <row
                    jcr:primaryType="cpp:Container"
                    sling:resourceType="composum/pages/components/container/row"
                    columns="-6--6-">
                <column-0
                        jcr:primaryType="cpp:Container"
                        sling:resourceType="composum/pages/components/container/row/column"/>
                <column-1
                        jcr:primaryType="cpp:Container"
                        sling:resourceType="composum/pages/components/container/row/column">
                    <text1
                            jcr:primaryType="cpp:Element"
                            sling:resourceType="composum/pages/components/element/text"
                            text="&lt;p>If you need support with Composum, you are welcome to contact us at&amp;nbsp;&lt;a href=&quot;mailto:support@composum.com&quot; target=&quot;_blank&quot;>support@composum.com&lt;/a>.&lt;/p>"
                            title="Email"/>
                    <text
                            jcr:primaryType="cpp:Element"
                            sling:resourceType="composum/pages/components/element/text"
                            text="&lt;p>&amp;nbsp;If you feel the answer would be of general interest, you could also consider asking the question on &lt;a href=&quot;https://stackoverflow.com/&quot; target=&quot;_blank&quot;>stackoverflow.com&lt;/a> with a tag composum and alert us about that.&amp;nbsp;&lt;/p>"
                            title="Stackoverflow"/>
                </column-1>
            </row>
        </main>
        <bottom
                jcr:primaryType="cpp:Container"
                sling:resourceType="composum/pages/components/container/iparsys"/>
        <navigation
                jcr:primaryType="nt:unstructured"
                title="Cont@ct"/>
        <style
                jcr:primaryType="nt:unstructured"/>
    </jcr:content>
</jcr:root>
