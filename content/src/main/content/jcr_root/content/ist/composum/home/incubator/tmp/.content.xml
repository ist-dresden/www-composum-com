                            jcr:primaryType="cpp:Element"
                            sling:resourceType="composum/pages/components/element/text"
                            text="&#xa;                &lt;ul>&#xa;                &#xa;                    &lt;li>&#xa;                    Overlays the property crop.vertical for the wide variation to ensure that the visible detail looks good for this image.&#xa;                    &lt;/li>&#xa;                &#xa;                &lt;/ul>&#xa;            "
                            textAlignment="left"/>
